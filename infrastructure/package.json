{
  "name": "infrastructure",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "pnpm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "pnpm i cdktf@next cdktf-cli@next",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run format",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.864.0",
    "@cdktf/provider-aws": "21.8.0",
    "@cdktf/provider-mongodbatlas": "^8.4.0",
    "@cdktf/provider-random": "^12.0.0",
    "aws-lambda": "^1.0.7",
    "cdktf": "^0.21.0",
    "cdktf-cli": "^0.21.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "dotenv": "^17.2.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}
