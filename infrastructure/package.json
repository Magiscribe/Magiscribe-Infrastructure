{
  "name": "infrastructure",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "pnpm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "pnpm i cdktf@next cdktf-cli@next",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run format",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@cdktf/provider-aws": "19.46.0",
    "@cdktf/provider-mongodbatlas": "^7.8.0",
    "@cdktf/provider-random": "^11.0.3",
    "aws-lambda": "^1.0.7",
    "cdktf": "^0.20.10",
    "cdktf-cli": "^0.20.10",
    "constructs": "^10.4.2",
    "esbuild": "^0.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@types/aws-lambda": "^8.10.146",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
